{
  "id": "waltemarapp",
  "name": "Waltemar Carneiro",
  "short_name": "WaltemarApp",
  "description": "Web Developments",
  "categories": ["entertainment", "productivity"],
  "lang": "pt-BR",
  "dir": "ltr",
  "default_locale": "pt-BR",
  "display": "standalone",
  "theme_color": "#f1f2f4",
  "background_color": "#f1f2f4",
  "orientation": "portrait",
  "start_url": "/",
  "scope": "/",
  "capture_links": "existing-client-navigate",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "offline_enabled": true,
  "iarc_rating_id": "ESRB_E",
  "prefer_related_applications": false,
  "protocol_handlers": [],
  "related_applications": [],
  "handle_links": "preferred",
  "display_override": ["window-controls-overlay"],
  "window-controls-overlay": {
    "type": "overlay",
    "settings": {
      "color": "#120912",
      "foreground": "#ffffff"
    }
  },
  "share_target": {
    "action": "/shared-content-receiver/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/pdf": [".pdf"],
        "image/png": [".png"],
        "image/jpeg": [".jpg", ".jpeg"]
      }
    }
  ],
  "scope_extensions": [
    { "origin": "https://www.youtube.com" },
    { "origin": "https://youtube.com" }
  ],
  "icons": [
    { "src": "icons/icon48.png", "sizes": "48x48", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon48.png", "sizes": "48x48", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon72.png", "sizes": "72x72", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon72.png", "sizes": "72x72", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon96.png", "sizes": "96x96", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon96.png", "sizes": "96x96", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon144.png", "sizes": "144x144", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon144.png", "sizes": "144x144", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon152.png", "sizes": "152x152", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon152.png", "sizes": "152x152", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon192.png", "sizes": "192x192", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon192.png", "sizes": "192x192", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon256.png", "sizes": "256x256", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon256.png", "sizes": "256x256", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon384.png", "sizes": "384x384", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon384.png", "sizes": "384x384", "type": "image/png", "purpose": "maskable" },
    { "src": "icons/icon512.png", "sizes": "512x512", "type": "image/png", "purpose": "any" },
    { "src": "icons/icon512.png", "sizes": "512x512", "type": "image/png", "purpose": "maskable" }
  ],
  "shortcuts": [
    {
      "name": "Contribuir",
      "short_name": "PIX",
      "description": "Fazer uma contribuição via PIX",
      "url": "/bank/bb.html",
      "icons": [{ "src": "shortcuts/pix.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    },
    {
      "name": "Rádio",
      "short_name": "Play",
      "description": "Continuar ouvindo",
      "url": "/radio/index.html?action=play&last=true",
      "icons": [{ "src": "shortcuts/playlist.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    },
    {
      "name": "Play List",
      "short_name": "PlayList",
      "description": "Continuar assistindo",
      "url": "/youtube/index.html?continue=true&from=shortcut",
      "icons": [{ "src": "shortcuts/filme.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    },
    {
      "name": "Menu Principal",
      "short_name": "Config",
      "description": "Acessar configurações do app",
      "url": "/home.html#options",
      "icons": [{ "src": "shortcuts/menu.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    },
    {
      "name": "Buscar",
      "short_name": "Busca",
      "description": "Pesquisar no app",
      "url": "/search.html",
      "icons": [{ "src": "shortcuts/search.svg", "sizes": "96x96", "type": "image/svg+xml" }]
    }
  ],
  "screenshots": [
    { "src": "screenshots/screenshot-desktop-wide.jpg", "sizes": "1920x1080", "type": "image/jpg", "form_factor": "wide" },
    { "src": "screenshots/1.jpg", "sizes": "1080x1920", "type": "image/jpg" },
    { "src": "screenshots/3.jpg", "sizes": "1080x1920", "type": "image/jpg" },
    { "src": "screenshots/screenshot-mobile.jpg", "sizes": "1080x1920", "type": "image/jpg", "form_factor": "narrow" },
    { "src": "screenshots/screenshot-desktop-wide2.jpg", "sizes": "1920x1080", "type": "image/jpg", "form_factor": "wide" }
  ],
  "widgets": [
    {
      "name": "Rádio Player",
      "short_name": "Player",
      "description": "Controle rápido do player de rádio",
      "tag": "radio-widget",
      "url": "/widgets/radio-player.html",
      "update_frequency": "hourly",
      "icons": [{ "src": "icons/widget-radio.png", "sizes": "96x96", "type": "image/png" }],
      "screenshots": [{ "src": "screenshots/widget-radio.png", "sizes": "400x300", "type": "image/png" }]
    },
    {
      "name": "Love Songs Status",
      "short_name": "Love Songs",
      "description": "Veja as músicas mais pedidas",
      "tag": "love-songs-widget",
      "url": "/widgets/love-songs.html",
      "update_frequency": "daily",
      "icons": [{ "src": "icons/widget-love.png", "sizes": "96x96", "type": "image/png" }],
      "screenshots": [{ "src": "screenshots/widget-love.png", "sizes": "400x300", "type": "image/png" }]
    },
    {
      "name": "Atalhos Rápidos",
      "short_name": "Atalhos",
      "description": "Acesso rápido às funcionalidades principais",
      "tag": "quick-actions",
      "url": "/widgets/quick-actions.html",
      "update_frequency": "daily",
      "icons": [{ "src": "icons/widget-shortcuts.png", "sizes": "96x96", "type": "image/png" }],
      "screenshots": [{ "src": "screenshots/widget-shortcuts.png", "sizes": "400x300", "type": "image/png" }]
