{
  "id": "waltemarapp",
  "name": "Waltemar Carneiro",
  "short_name": "Waltemar",
  "description": "Experiência digital integrada com rádio online, vídeos e conteúdos interativos",
  "lang": "pt-BR",
  "dir": "ltr",
  "start_url": "/index.html",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "tabbed"],
  "offline_enabled": true,
  "background_color": "#f1f2f4",
  "theme_color": "#f1f2f4",
  "orientation": "portrait",
  "iarc_rating_id": "SEU-CODIGO-IARC-AQUI",
  "scope": "/",
  "scope_extensions": [
    { "origin": "https://waltemar.com.br" },
    { "origin": "https://www.waltemar.com.br" }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "capture_links": "existing-client-navigate",
  "handle_links": "preferred",
  "protocol_handlers": [],
