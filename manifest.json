    "entertainment",
    "productivity"
  ],
  "share_target": {
    "action": "/shared-content-receiver/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display_override": [
    "window-controls-overlay"
  ],
  "window-controls-overlay": {
    "type": "overlay",
    "settings": {
      "color": "#120912",
      "foreground": "#ffffff"
    }
  },
  "handle_links": "preferred",
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/pdf": [".pdf"],
        "image/png": [".png"],
        "image/jpeg": [".jpg", ".jpeg"]
      }
    }
  ],
  "widgets": [
    {
      "name": "Rádio Player",
      "short_name": "Player",
      "description": "Controle rápido do player de rádio",
      "tag": "radio-widget",
      "url": "/widgets/radio-player.html",
      "update_frequency": "hourly",
      "icons": [
        {
          "src": "icons/widget-radio.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "screenshots": [
        {
          "src": "screenshots/widget-radio.png",
          "sizes": "400x300",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Love Songs Status",
      "short_name": "Love Songs",
      "description": "Veja as músicas mais pedidas",
