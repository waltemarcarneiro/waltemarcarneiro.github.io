  "shortcuts": [
    {
      "name": "Contribuir",
      "description": "Fazer uma contribuição via PIX",
      "url": "/bank/bb.html",
      "icons": [
        {
          "src": "shortcuts/pix.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ],
      "short_name": "PIX"
    },
    {
      "name": "Rádio",
      "url": "/radio/index.html?action=play&last=true",
      "description": "Continuar ouvindo",
      "icons": [
        {
          "src": "shortcuts/playlist.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ],
      "short_name": "Play"
    },
    {
      "name": "Play List",
      "url": "/youtube/index.html?continue=true&from=shortcut",
      "description": "Continuar assistindo",
      "icons": [
        {
          "src": "shortcuts/filme.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ],
