      "url": "/widgets/love-songs.html",
      "update_frequency": "daily",
      "icons": [
        {
          "src": "icons/widget-love.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "screenshots": [
        {
          "src": "screenshots/widget-love.png",
          "sizes": "400x300",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Atalhos Rápidos",
      "short_name": "Atalhos",
      "description": "Acesso rápido às funcionalidades principais",
      "tag": "quick-actions",
      "url": "/widgets/quick-actions.html",
      "update_frequency": "daily",
      "icons": [
        {
          "src": "icons/widget-shortcuts.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "screenshots": [
        {
          "src": "screenshots/widget-shortcuts.png",
          "sizes": "400x300",
          "type": "image/png"
        }
      ]
    }
