  "shortcuts": [
    { "name": "Contribuir", "short_name": "PIX", "description": "Fazer uma contribuição via PIX", "url": "/bank/brb.html", "icons": [{ "src": "shortcuts/pix.svg", "sizes": "96x96", "type": "image/svg+xml" }] },
    { "name": "Rádio", "short_name": "Play", "description": "Continuar ouvindo", "url": "/radio/index.html?action=play&last=true", "icons": [{ "src": "shortcuts/playlist.svg", "sizes": "96x96", "type": "image/svg+xml" }] },
